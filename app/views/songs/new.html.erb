<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />
  <p>Enter Notes about Song</p>
 
  <input type='text' id="song_notes_attributes_0_content" name='song[notes_attributes[]'>
  <input type='text' id="song_notes_attributes_1_content" name='song[notes_attributes][]'>
  <input type='text' id="song_notes_attributes_2_content" name='song[notes_attributes][]'>
  <%= f.submit %>
<% end %>


